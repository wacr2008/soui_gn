<?xml version="1.0" encoding="utf-8"?>
<UIDEF>
  <font face="宋体" size="13"/>

 
  <string>
    <!--需要修改的-->
    <title value="悟净营销大师beta"/>
    <ver value="1.0"/>
  </string>

  <skin>
	  <imglist name="skin_traymenu_icons"	src="img:traymenu_icons" states="9"/>
    <imgframe name="img_bg" src="IMG.MAIN:bg"/>
    <imglist name="img_welcome_center" src="IMG.MAIN:welcome_center"/>
    <imglist name="img_install" src="IMG.MAIN:install" states="4"/>
    <imglist name="img_custom_button" src="IMG.MAIN:custom_button" states="4"/>
  
    <imgframe name="prog_bar" src="IMG.MAIN:prog_bar" left="10" top="2" right="5" bottom="2"/>
    <imgframe name="prog_bkgnd" src="IMG.MAIN:prog_bkgnd" left="10" top="2" right="10" bottom="2"/>

    <imglist name="img_setup1" src="IMG.MAIN:setup1"/>
    <imglist name="img_setup2" src="IMG.MAIN:setup2"/>
    <imglist name="img_setup3" src="IMG.MAIN:setup3"/>
    <imglist name="img_setup4" src="IMG.MAIN:setup4"/>
    <imglist name="img_setup_tab" src="IMG.MAIN:setup_tab" states="4"/>

    <imglist name="img_finish_ads" src="IMG.MAIN:finish_ads" states="4"/>
    <imglist name="img_finish_feel" src="IMG.MAIN:finish_feel" states="4"/>
    <imglist name="img_finish_finish" src="IMG.MAIN:finish_finish" states="4"/>
  </skin>

  <style>
    <class name="normalbtn" font="" colorText="#385e8b" colorTextDisable="#91a7c0" textMode="25" cursor="hand" margin-x="0"/>
    <class name="cls_btn_weblink" cursor="hand" colorText="#1e78d5" colorTextHover="#1e78d5" font="italic:0" fontHover="underline:1,italic:0" />
    
    <class name="css_imgbtn" font="" colorText="#6C8BA9" colorTextDisable="#91a7c0" cursor="hand"/>
    <class name="css_chk" font="" colorText="#6C8BA9" colorTextDisable="#91a7c0" cursor="hand"/>
    <class name="css_text" font="" colorText="#6C8BA9" colorTextDisable="#91a7c0" cursor="hand"/>

    <class name="css_edit_window"  colorBorder="#6C8BA9" valign="middle" margin-x="1" margin-y="1" />
    <class name="css_edit" textmode="24"  colorText="#000000"/>
  </style>

  <objattr>
    <imgbtn class="css_imgbtn"/>
    <check  class="css_chk"/>
    <link class="cls_btn_weblink"/>
    <text class="css_text"/>
    <edit class="css_edit"/>
    <progress  bkgndSkin="prog_bkgnd" posSkin="prog_bar" value="20" min="0" max="100"/>
  </objattr>
</UIDEF>